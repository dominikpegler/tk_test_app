   "source": [
    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "query = '''\n",
    "\n",
    "SELECT name FROM sqlite_master\n",
    "WHERE type='table'\n",
    "ORDER BY name;\n",
    "\n",
    "'''\n",
    "\n",
    "conn = sqlite3.connect(\"sqlite.db\")\n",
    "df = pd.read_sql(query, conn)\n",
    "conn.close()\n",
    "\n",
    "df\n"
   ]
  }
